{
  "version": 2,
  "alias": [
    "open-design.vercel.app",
    "opn-design.vercel.app",
    "opend.vercel.app",
    "opnd.vercel.app",
    "opndx.vercel.app"
  ],
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": true
}
